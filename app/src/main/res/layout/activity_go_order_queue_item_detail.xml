        <View
            android:layout_width="1dp"
            android:background="@color/colorBackGround"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="18dp">
        </View>
        <TextView
            android:textSize="16sp"
